#include<iostream>
using namespace std;

int main() {
    int b, n, m;
    long long result = 1;
    cout << "Enter the value of b" << endl;
    cin >> b;
    cout << "Enter the value of n" << endl;
    cin >> n;
    cout << "Enter the value of m" << endl;
    cin >> m;
    b = b % m;
    for(int i = 0; i < n; i++) {
        result = (result * b) % m;
    }
    cout << "b^n mod m is " << result << endl;
}
