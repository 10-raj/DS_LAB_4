#include <iostream>
using namespace std;

int fibonacci(int n) {
    if (n <= 1)
        return n;
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int terms;
    cout << "Enter the number of terms" << endl;
    cin >> terms;

    cout << "Fibonacci series:" << endl;
    for (int i = 1; i < terms; i++) {
        cout << fibonacci(i) << " ";
    }
    cout << endl;
}
